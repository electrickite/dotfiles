[general]
status_path = "~/.cache/vdirsyncer/status/"

[pair contacts]
a = "contacts_local"
b = "contacts_remote"
collections = ["from a", "from b"]

[storage contacts_local]
type = "filesystem"
path = "~/.contacts/"
fileext = ".vcf"

[storage contacts_remote]
type = "carddav"
url = "https://example.com/"
username = "myuser"
password.fetch = ["command", "pass", "line", "my/password"]
